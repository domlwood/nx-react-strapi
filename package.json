{
  "name": "@portal/source",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "apps/**"
  ],
  "scripts": {
    "client": "nx serve portal-client",
    "db": "docker-compose --env-file .env -f docker-compose.yml up",
    "init": "cp .env.example .env",
    "lint": "nx lint portal-client",
    "server": "concurrently \"npm run db\" \"nx build portal-cms && nx serve portal-cms\"",
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "test": "npx nx run-many --all --target=test --watch --skip-nx-cache"
  },
  "private": true,
  "dependencies": {
    "@swc/helpers": "~0.5.0",
    "concurrently": "^8.2.0",
